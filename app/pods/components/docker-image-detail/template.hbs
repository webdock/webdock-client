<dl>
  <dt>{{t 'docker-image-detail.repository'}}</dt>
  <dd>{{dockerImage.repoTags}}</dd>
  <dt>{{t 'docker-image-detail.created'}}</dt>
  <dd>{{dockerImage.created}}</dd>
  <dt>{{t 'docker-image-detail.size'}}</dt>
  <dd>{{dockerImage.size}}</dd>
  <dt>{{t 'docker-image-detail.virtual-size'}}</dt>
  <dd>{{dockerImage.virtualSize}}</dd>
  {{#if dockerImage.parentId}}
    <dt>{{t 'docker-image-detail.parent-id'}}</dt>
    <dd>{{dockerImage.parentId}}</dd>
  {{/if}}
  <dt>{{t 'docker-image-detail.id'}}</dt>
  <dd>{{dockerImage.id}}</dd>
</dl>
